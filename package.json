{
	"name": "ultimate-blocks",
	"version": "3.0.1",
	"private": true,
	"scripts": {
		"start": "concurrently -k -c 'blue,yellow,green' -n 'gutenberg,recompile,bundle' \"wp-scripts start\" \"node recompileOnChange.js\" \"npm run bundle_additional_watch\"",
		"bundle_additional_watch": "parcel watch ./admin/js/src/*.js --dist-dir ./bundle-dist --no-hmr",
		"bundle_additional_build": "parcel build ./admin/js/src/*.js --dist-dir ./bundle-dist --no-cache --no-source-maps",
		"build": "wp-scripts build && node recompile.js && npm run bundle_additional_build && npm run compress-plugin-prod",
		"compress-plugin-dev": "cross-env NODE_ENV=development node ./pluginCompress.js ultimate-blocks.zip",
		"compress-plugin-prod": "cross-env NODE_ENV=production node ./pluginCompress.js ultimate-blocks.zip"
	},
	"alias": {
		"$Containers": "./admin/js/src/containers",
		"$Components": "./admin/js/src/components",
		"$HOC": "./admin/js/src/hoc",
		"$Stores": "./admin/js/src/stores",
		"$Styles": "./admin/css/src/",
		"$Library": "./library"
	},
	"dependencies": {
		"@babel/core": "^7.18.2",
		"@babel/plugin-proposal-class-properties": "^7.17.12",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@fortawesome/fontawesome-svg-core": "^6.3.0",
		"@fortawesome/free-brands-svg-icons": "^6.3.0",
		"@fortawesome/free-solid-svg-icons": "^6.3.0",
		"@fortawesome/react-fontawesome": "^0.2.0",
		"@reduxjs/toolkit": "^1.8.4",
		"@wordpress/api-fetch": "^6.27.0",
		"@wordpress/block-editor": "^11.2.0",
		"@wordpress/blocks": "^11.15.0",
		"@wordpress/components": "^23.2.0",
		"@wordpress/compose": "^6.5.0",
		"@wordpress/data": "^7.0.0",
		"@wordpress/hooks": "^3.16.0",
		"@wordpress/i18n": "^4.15.0",
		"deepmerge": "^4.2.2",
		"moment": "^2.29.4",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-redux": "^8.0.2",
		"react-sortable-hoc": "^2.0.0",
		"react-transition-group": "^4.4.5",
		"swiper": "^6.8.4",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.18.9",
		"@babel/preset-react": "^7.18.6",
		"@parcel/transformer-sass": "^2.7.0",
		"@wordpress/eslint-plugin": "^13.0.0",
		"@wordpress/scripts": "^23.2.0",
		"archiver": "^5.3.1",
		"concurrently": "^7.2.1",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.1",
		"eslint-import-resolver-custom-alias": "^1.3.0",
		"eslint-import-resolver-webpack": "^0.13.2",
		"fancy-terminal": "^1.0.1",
		"gulp": "^4.0.2",
		"gulp-notify": "^4.0.0",
		"gulp-sort": "^2.0.0",
		"gulp-wp-pot": "^2.5.0",
		"ignore-emit-webpack-plugin": "^2.0.6",
		"mini-css-extract-plugin": "^2.6.0",
		"parcel": "^2.7.0",
		"path": "^0.12.7",
		"postcss-loader": "^7.0.0",
		"postcss-preset-env": "^7.7.0",
		"sass": "^1.52.1",
		"sass-loader": "^13.0.0"
	}
}